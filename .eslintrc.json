{
  "extends": "airbnb",
  "rules" : {
	"indent": [1, "tab"],
	"no-tabs": 0,
	"no-mixed-spaces-and-tabs" : 0,
	"no-plusplus": 0,
	"no-restricted-syntax" : 0,
	"class-methods-use-this" : 0,
	"consistent-return" : 0,
	"no-eval" : 0,
	"quote-props" : 0,
	"key-spacing" : 0,
	"object-shorthand" : 0,
	"guard-for-in" : 0,
	"import/no-unresolved" : 0,
	"global-require" : 0,
	"new-cap": 0,
	"camelcase" : 0,
	"no-undef" : 0,
	"no-param-reassign" : 0,
	"no-sequences" : 0,
	"no-cond-assign" : 0,
	"prefer-template" : 0,
	"prefer-arrow-callback" : 0,
	"padded-blocks" : 0

  }

  //    "plugins": [
  //        "react",
  //        "jsx-a11y",
  //        "import"
  //    ]
}
